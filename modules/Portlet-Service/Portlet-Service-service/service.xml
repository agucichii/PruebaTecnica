<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="com.ricoh">
	<namespace>agu</namespace>
	
	<entity local-service="true" name="Registro" remote-service="true" uuid="true">

		<column name="registroId" primary="true" type="long" />
		<column name="nombre" type="String" />
		<column name="apellido" type="String" />
		<column name="nacimiento" type="Date" />
		<column name="correo" type="String" />
		<column name="fechacreacion" type="Date" />

	</entity>
</service-builder>